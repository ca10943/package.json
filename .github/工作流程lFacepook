# Step 1: Authenticate
$ cat ~/GH_TOKEN.txt | docker login docker.pkg.github.com -u ca10943 --password-stdin
# Step 2: Tag
$ docker tag IMAGE_ID docker.pkg.github.com/ca10943/rwseo.j-query/IMAGE_NAME:VERSION
# Step 3: Publish
$ docker push docker.pkg.github.com/ca10943/rwseo.j-query/IMAGE_NAME:VERSION
